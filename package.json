{
  "name": "comfy-browser",
  "productName": "Comfy Image Browser",
  "private": true,
  "version": "0.1.0",
  "main": "electron-dist/electron/main.js",
  "scripts": {
  "dev": "npm run build:electron && concurrently --kill-others --success=first \"npm:dev:app\" \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "tsc -w -p electron/tsconfig.json",
    "dev:app": "wait-on http://localhost:5173 && VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "npm run build:renderer && npm run build:electron",
    "build:renderer": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "preview": "vite preview",
    "test": "vitest run --environment node"
  },
  "dependencies": {
    "idb": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^30.0.9",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^1.6.0",
    "wait-on": "^7.2.0"
  }
}
